\n        animationDuration: `.${num}s`,\n        zIndex: \n            (!drag && vValue) || \n            (drag && 100000),\n    }\n\n    const handleDrag = (e, ui) => {\n        const X = document.body.clientWidth-200\n        const Y = window.innerHeight-400\n\n        if(e.screenX > X && e.screenY < Y) close(num)\n    }\n\n    return (\n        <Draggable \n            onStart={() => {\n                order(num)\n                setDrag(true)\n            }}\n            onDrag={handleDrag}\n            onStop={() => setDrag(false)}\n            defaultPosition={{ \n                x: tape!== null ? xy : xy + 80,\n                y: tape!== null ? xy : xy + 200\n            }}>\n            <div \n                style={allStyle}\n                onMouseEnter={() => setHover(true)}\n                onMouseLeave={() => setHover(false)}\n            > \n                {tape !== null ? <div style={tapeStyle}/>: null}\n                <div style={divStyle}></div>\n            </div>\n        </Draggable>\n    );\n}\n\n\nexport default AboutMe;","/Users/user/Documents/GitHub/portfolio/src/components/Sub/Tags.jsx",[],{"ruleId":"115","replacedBy":"116"},{"ruleId":"117","replacedBy":"118"},{"ruleId":"119","severity":1,"message":"120","line":98,"column":13,"nodeType":"121","messageId":"122","endLine":98,"endColumn":16},{"ruleId":"123","severity":1,"message":"124","line":1,"column":8,"nodeType":"121","messageId":"125","endLine":1,"endColumn":14},{"ruleId":"123","severity":1,"message":"126","line":3,"column":10,"nodeType":"121","messageId":"125","endLine":3,"endColumn":14},{"ruleId":"123","severity":1,"message":"127","line":3,"column":16,"nodeType":"121","messageId":"125","endLine":3,"endColumn":21},{"ruleId":"123","severity":1,"message":"128","line":8,"column":8,"nodeType":"121","messageId":"125","endLine":8,"endColumn":13},{"ruleId":"123","severity":1,"message":"128","line":4,"column":8,"nodeType":"121","messageId":"125","endLine":4,"endColumn":13},"no-native-reassign"