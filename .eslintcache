ayoutRatio[\"sub\"]}%;\n\n    button {\n        margin-left: ${({active}) => active ? \"-6\" : \"-4\"}rem;\n        position: relative;\n        padding: .2rem;\n        width: 100%;\n        background-color: white;\n        box-sizing: border-box;\n        box-shadow: 0rem 0rem .15rem rgba(0, 0, 0, .5);\n        transition: all .2s;\n\n        &:before {\n            content: \"${({content}) => content}\";\n            height: 1.2rem;\n            display: block;\n            background-color: white;\n            border: .3rem double #F72D0D;\n            font-size: 1rem;\n            font-weight: 400;\n            font-style: italic;\n            white-space: nowrap;\n            letter-spacing: -.05rem;\n            padding-left: .1rem;\n            padding-bottom: .3rem;\n        }\n    }\n\n    &:hover button {\n        margin-left: -6.5rem;\n        box-shadow: .1rem .1rem .15rem rgba(0, 0, 0, .3);\n    }\n    &:active button {\n        margin-left: -5rem;\n        box-shadow: .1rem .1rem .15rem rgba(0, 0, 0, .3);\n    }\n\n    &:hover .hoverOn {\n        display: block;\n        position: absolute;\n        background-color: black;\n        color: white;\n        font-size: 1.2rem;\n        line-height: 1rem;\n        padding: .2rem .15rem .2rem .15rem;\n        right: 15.5rem;\n        top: .3rem;\n    }\n`;\nconst value = 200\n\nclass Menu extends Component {\n    constructor(props){\n        super(props)\n        this.state = {\n            menu: {}, \n            aboutMe: {},\n            cor: {}\n        }\n        this.postItHandler = this.postItHandler.bind(this)\n        this.postItDouble = this.postItDouble.bind(this)\n        this.postItReorder = this.postItReorder.bind(this)\n        this.postItClose = this.postItClose.bind(this)\n    }\n\n    componentDidMount(){\n        this._menu()\n        this._aboutMe()\n\n        this.setState({ cor : {\n                clientWidth : document.body.clientWidth,\n                X: document.body.clientWidth-